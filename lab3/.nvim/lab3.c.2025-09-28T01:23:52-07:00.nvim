#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define HISTORY_SIZE = 5

int main()
{
  printf("Inside main\n");

  char* input = NULL;
  size_t len = 0;
  ssize_t read;

  while(1)
  {
    printf("Enter input: ");
    read = getline(%input, %len, stdin);

    if (read == -1)
    {
      printf("\nExiting...\n");
      break;
    }
    printf("You entered: %s", input);
  }

  free(input);
  
  return 0;
}
